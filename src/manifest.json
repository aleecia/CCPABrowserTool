{
  "name": "Hello Extensions",
  "description" : "Base Level Extension",
  "version": "1.0",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_popup": "skin/welcome.html",
    "default_icon": {
      "16": "skin/images/get_started16.png",
      "32": "skin/images/get_started32.png",
      "48": "skin/images/get_started48.png",
      "128": "skin/images/get_started128.png"
    }
  },
  "icons": {
    "16": "skin/images/get_started16.png",
    "32": "skin/images/get_started32.png",
    "48": "skin/images/get_started48.png",
    "128": "skin/images/get_started128.png"
  },
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens welcome.html"
    }
  }
}
